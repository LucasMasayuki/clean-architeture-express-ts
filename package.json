{
  "name": "clean-architeture-express-ts",
  "version": "1.0.0",
  "description": "Template to clean architeture express typescript app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucasMasayuki/clean-architeture-express-ts.git"
  },
  "keywords": [
    "TypeScript",
    "express",
    "nodejs",
    "jest",
    "eslint"
  ],
  "author": "Lucas Masayuki",
  "main": "dist/server.js",
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig-build.json",
    "start": "node -r dotenv/config dist/main/server.js",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "build:watch": "rimraf dist && tsc -p tsconfig-build.json -w",
    "check": "npm-check -s -u",
    "dev": "npm run build && nodemon",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts tests/**/*.ts --fix",
    "pre-commit": "lint-staged",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.js",
    "test": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "dependencies": {
    "@graphql-tools/schema": "8.3.1",
    "@graphql-tools/utils": "8.5.3",
    "@types/node-xlsx": "^0.15.2",
    "apollo-server-express": "3.5.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "csurf": "1.11.0",
    "date-fns": "^2.29.1",
    "debug": "4.3.2",
    "dotenv": "10.0.0",
    "eslint-plugin-promise": "5.1.0",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "helmet": "4.6.0",
    "http-errors": "1.8.0",
    "jsonwebtoken": "8.5.1",
    "module-alias": "2.2.2",
    "morgan": "1.10.0",
    "mssql": "7.2.1",
    "multer": "1.4.3",
    "node-cron": "^3.0.2",
    "node-xlsx": "^0.17.2",
    "nodemon": "2.0.12",
    "pg": "8.7.1",
    "sha256": "0.2.0",
    "swagger-jsdoc": "5.0.1",
    "swagger-ui-express": "^4.6.2",
    "ts-node": "10.1.0",
    "typeorm": "0.2.40",
    "uuid": "8.3.2",
    "validator": "13.6.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@jest-mock/express": "1.4.5",
    "@types/bcrypt": "5.0.0",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/csurf": "1.11.2",
    "@types/express": "4.17.13",
    "@types/faker": "5.5.9",
    "@types/http-errors": "1.8.1",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "8.5.6",
    "@types/module-alias": "2.0.1",
    "@types/mongodb": "4.0.7",
    "@types/morgan": "1.9.3",
    "@types/mssql": "7.1.4",
    "@types/multer": "1.4.7",
    "@types/node-cron": "^3.0.7",
    "@types/sha256": "0.2.0",
    "@types/supertest": "2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-dist": "^3.30.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "8.3.1",
    "@types/validator": "13.7.0",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "apollo-server-integration-testing": "3.0.0",
    "commitlint": "^17.6.1",
    "eslint": "8.2.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "faker": "5.5.3",
    "husky": "7.0.4",
    "inversify": "^6.0.1",
    "jest": "27.3.1",
    "jest-mock": "27.3.0",
    "lint-staged": "12.0.2",
    "mockdate": "3.0.5",
    "rimraf": "3.0.2",
    "superagent": "6.1.0",
    "supertest": "6.1.6",
    "swagger-ui-dist": "^5.0.0-alpha.3",
    "ts-jest": "27.0.7",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.4"
  }
}
